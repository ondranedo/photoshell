function psi() {
    if [[ "$1" != "r" ]]; then
        ~/psi/build-linux.sh ~/psi
        echo "[psi()] Build finished..."
    fi
    
    ~/psi/build/linux/Debug/testbed 
}

function photoshell() {
    ~/dev/photoshell/linux-build.sh ~/dev/photoshell/
    valgrind --leak-check=full --show-leak-kinds=all --track-origins=yes --verbose --log-file=valgrind-out.txt ~/dev/photoshell/build/Debug/photoshell -i ~/dev/photoshell/img/input.tga ~/dev/photoshell/img/output.tga

}

function capslocktoesc() {
    ./func/capslocktoesc.zsh
}

export EDITOR=nvim
